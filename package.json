{
  "name": "custom-nativescript-oauth2",
  "version": "1.0.0",
  "description": "OAuth 2 generic authorization plugin for NativeScript that doesn't install third party native libraries",
  "main": "dist/index.js",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "5.2.0",
      "ios": "5.2.0"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "prepare": "npm run build",
    "build": "webpack src/index.js dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexziskind1/nativescript-oauth2.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "TypeScript",
    "Office365",
    "Facebook",
    "Google",
    "Azure AD",
    "iOS",
    "Android"
  ],
  "author": "Alexander Ziskind <alex@nuvious.com> (https://nativescripting.com)",
  "bugs": {
    "url": "https://github.com/alexziskind1/nativescript-oauth2/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/alexziskind1/nativescript-oauth2",
  "devDependencies": {
    "tns-core-modules": "^5.2.0",
    "tns-platform-declarations": "^5.2.0",
    "typescript": "3.1.1",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "tslint": "5.18.0",
    "semver": "^5.5.0"
  },
  "dependencies": {
    "querystring": "0.2.0",
    "url": "0.11.0"
  },
  "bootstrapper": "nativescript-plugin-seed"
}
